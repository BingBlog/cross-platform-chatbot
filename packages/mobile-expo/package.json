{
  "name": "@chatbot/mobile-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "Expo React Native mobile application for AI chatbot",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "android-fix": "./launch-android.sh",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:debug": "node ./scripts/debug-web.js",
    "web:info": "node ./scripts/debug-web.js --info",
    "web:check": "node ./scripts/debug-web.js --check",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export --platform web",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "type-check": "tsc --noEmit",
    "lint": "expo lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.2",
    "expo": "~54.0.13",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.11",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "keywords": [
    "expo",
    "react-native",
    "mobile",
    "chatbot",
    "ai",
    "cross-platform"
  ],
  "private": true
}
